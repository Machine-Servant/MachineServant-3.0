    "gatsby-source-filesystem": "2.7.0",
    "gatsby-transformer-sharp": "2.8.0",
    "husky": "4.3.5",
    "lint-staged": "10.5.3",
    "postcss": "8.1.14",
    "prettier": "2.2.1",
    "tailwindcss": "2.0.1",
    "typescript": "4.1.2"
  },
  "keywords": [
    "gatsby",
    "typescript",
    "MachineServant"
  ],
  "license": "UNLICENSED",
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Machine-Servant/MachineServant-3.0"
  },
  "bugs": {
    "url": "https://github.com/Machine-Servant/MachineServant-3.0/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
